package main

import (
	"github.com/akrennmair/pascal/pas2go/system"
)

var _ = system.Write

// program ftoc
func main() {
	var (
		fgr float64
		cgr float64
	)
	_ = fgr
	_ = cgr

	system.Write("Degree F: ")
	system.Readln(&fgr)
	cgr = (fgr - 32.0e0) * 5.0e0 / 9.0e0
	system.Writeln("Degree C: ", cgr)
}
